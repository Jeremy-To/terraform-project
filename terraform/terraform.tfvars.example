# Terraform Variables
# Copy this file to terraform.tfvars and fill in your values

project_id = "your-gcp-project-id"
region     = "us-central1"
zone       = "us-central1-a"

ssh_user             = "ubuntu"
ssh_public_key_path  = "~/.ssh/id_rsa.pub"
admin_ip_ranges      = ["YOUR_IP/32"]

# Update these after building Packer images
web_server_image = "projects/your-gcp-project-id/global/images/family/web-server"
app_server_image = "projects/your-gcp-project-id/global/images/family/app-server"
db_server_image  = "projects/your-gcp-project-id/global/images/family/db-server"
